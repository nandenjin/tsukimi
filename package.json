{
  "name": "tsukimi",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "description": "Useful toolkit for scrapbox.io as browser extension",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && eslint . --ext ts",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.{js,ts,tsx,jsx,json,md,yml,yaml}\"",
    "lintfix": "eslint . --ext ts --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "mermaid": "^10.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.23",
    "@types/chrome": "^0.0.262",
    "@types/jquery": "^3.5.16",
    "@types/node": "^20.2.5",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "typescript": "^5.0.2",
    "vite": "^4.5.3",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "lint-staged": {
    "*.{ts,tsx,ts}": "eslint --cache --fix",
    "*.{js,ts,tsx,jsx,json,md,yml,yaml}": "prettier --write"
  }
}
